<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">

 <title>Derek Eats Food - derekeatsfood.com</title>
 <link href="http://www.derekeatsfood.com/atom.xml" rel="self"/>
 <link href="http://www.derekeatsfood.com"/>
 <updated>{{ site.time | date_to_xmlschema }}</updated>
 <id>http://www.derekeatsfood.com/</id>
 <author>
   <name>Derek Guenther</name>
   <email>dguenther9@gmail.com</email>
 </author>

 {% for post in site.posts %}
 <entry>
   <title>{{ post.title }}</title>
   <link href="http://www.derekeatsfood.com{{ post.url }}"/>
   <updated>{{ post.date | date_to_xmlschema }}</updated>
   <id>http://www.derekeatsfood.com{{ post.id }}</id>
   <content type="html">{{ post.content | xml_escape }}</content>
 </entry>
 {% endfor %}

</feed>
